{
  "manifest_version": 2,
  "name": "Full Ads",
  "description": "Version 1.0",
  "version": "1.0",
  "icons": {
    "128": "/images/ads.png"
  },
  "background": {
    "scripts": [
      "js/ChromeEvents.js"
    ]
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/",
    "https://*/",
    "unlimitedStorage",
    "webRequest"
  ],
  "browser_action": {
    "default_title": "Full Ads",
    "default_icon": "/images/ads.png"
  }
}